create table annonse(
id integer not null,
title varchar(100) not null,
picturePath varchar(250) not null,
pictureAlt varchar(100) not null,
pictureCapt varchar(100) not null,
text varchar(21000) not null,
date DATETIME,
author varchar(50) not null,
category varchar(30) not null,
importance smallint not null
);
;



auto increment id og sett dato til current time stamp



Select overskrift, innhold, kategori from annonse where id ?

{"overskrift": "ny tittel", "innhold": "Nytt innhold", "Bilde":"", "kategori": "Sport", "viktighet":"1"

