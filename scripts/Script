create table annonse(
id integer not null,
overskrift varchar(100) not null,
bilde varchar(250) not null,
bildetekst varchar(100) not null,
bildealt varchar(100) not null,
innhold varchar(21000) not null,
kategori varchar(30) not null,
viktighet smallint not null,
forfatter varchar(50) not null,
dato DATETIME);
;

auto increment id og sett dato til current time stamp



Select overskrift, innhold, kategori from annonse where id ?

