create table annonse(
id integer not null,
overskrift varchar(100) not null,
innhold varchar(21000) not null,
bilde varchar(250) not null,
kategori varchar(30) not null,
viktighet smallint not null);

Verdier vi setter inn:
insert into annonse ( overskrift, innhold, bilde, kategori, viktighet) values
('tittel 1', 'tekst til nr 1', 'https://image.cnbcfm.com/api/v1/image/105992231-1561667465295gettyimages-521697453.jpeg?v=1561667497','Sport',1)
